class Solution{
   public static void shellSort(int[]arr){
     for(int gap=(arr.length/2);gap>0;gap=gap/2){
        for(int j=gap;j<arr.length;j++){
          for(int i=(j-gap);i>=0;i=i-gap){
             if(arr[i]<arr[i+gap]){
               break;
             }else{
                 int temp=arr[i];
                 arr[i]=arr[i+gap];
                 arr[i+gap]=temp;
             }
          }
        }
     }
   }
}
