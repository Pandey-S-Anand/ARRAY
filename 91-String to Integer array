class Solution {
   static ArrayList<Integer> arrayGenerator(String str){
	   	ArrayList<Integer>res=new ArrayList<Integer>();
	    int n=str.length(),num=0;
		
		  for(int i=0;i<n;i++)
		  {
		    char ch=str.charAt(i);
			
		    if(ch!=',')
			    num=(num*10)+ch-'0';
		    else
		    {
			    res.add(num);
			    num=0; 
		    }
		  }
    
		  res.add(num);
		
		 return res;
    }
}
